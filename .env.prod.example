# Copia a .env y ajusta antes de: docker compose -f docker-compose.prod.yml up -d
# cp .env.prod.example .env

APP_NAME="Pluss"
APP_ENV=production
APP_DEBUG=false
APP_URL=http://localhost:8000
APP_PORT=8000

# Dejar vac√≠o para que el entrypoint genere uno
